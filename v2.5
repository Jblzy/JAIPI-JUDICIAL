=====================================================================================================
FUNCTION: ASSESS_ADMISSIBILITY_AUSTRALIA(evidence_item, case_packet, jurisdiction_config)
=====================================================================================================

INPUT:
    evidence_item: EVIDENCE_OBJECT
    case_packet: CASE_PACKET
    jurisdiction_config: JURISDICTION_CONFIG

OUTPUT:
    admissibility_assessment: {
        admissible: BOOLEAN,
        reasons: ARRAY[STRING],
        exclusion_rule: STRING or NULL,
        exceptions_applied: ARRAY[STRING],
        limited_use: STRING or NULL,
        probative_value: FLOAT,
        prejudicial_effect: FLOAT,
        s137_analysis: S137_ANALYSIS or NULL,
        s138_analysis: S138_ANALYSIS or NULL,
        judicial_determination_required: BOOLEAN
    }

PROCESS:

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 1: RELEVANCE CHECK (s55)
// ═══════════════════════════════════════════════════════════════════════════════════════════

facts_in_issue = IDENTIFY_FACTS_IN_ISSUE(case_packet.charges)

relevance = ASSESS_RELEVANCE(
    evidence: evidence_item,
    facts_in_issue: facts_in_issue
)

IF relevance.relevant == FALSE:
    RETURN {
        admissible: FALSE,
        reasons: ["Evidence not relevant - s55 Evidence Act"],
        exclusion_rule: "RELEVANCE_s55",
        exceptions_applied: [],
        limited_use: NULL,
        probative_value: 0.0,
        prejudicial_effect: 0.0,
        s137_analysis: NULL,
        s138_analysis: NULL,
        judicial_determination_required: FALSE
    }

probative_value = relevance.probative_value

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 2: HEARSAY CHECK (s59)
// ═══════════════════════════════════════════════════════════════════════════════════════════

IF IS_HEARSAY(evidence_item):
    hearsay_exception = CHECK_HEARSAY_EXCEPTIONS(evidence_item, jurisdiction_config)
    
    IF hearsay_exception.applies == FALSE:
        RETURN {
            admissible: FALSE,
            reasons: ["Hearsay - s59 Evidence Act, no exception applies"],
            exclusion_rule: "HEARSAY_s59",
            exceptions_applied: [],
            limited_use: NULL,
            probative_value: probative_value,
            prejudicial_effect: 0.0,
            s137_analysis: NULL,
            s138_analysis: NULL,
            judicial_determination_required: hearsay_exception.uncertain
        }
    ELSE:
        exceptions_applied = [hearsay_exception.exception_section]

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 3: OPINION CHECK (s76)
// ═══════════════════════════════════════════════════════════════════════════════════════════

IF IS_OPINION(evidence_item):
    opinion_exception = CHECK_OPINION_EXCEPTIONS(evidence_item, jurisdiction_config)
    
    IF opinion_exception.applies == FALSE:
        RETURN {
            admissible: FALSE,
            reasons: ["Opinion evidence - s76 Evidence Act, no exception applies"],
            exclusion_rule: "OPINION_s76",
            exceptions_applied: [],
            limited_use: NULL,
            probative_value: probative_value,
            prejudicial_effect: 0.0,
            s137_analysis: NULL,
            s138_analysis: NULL,
            judicial_determination_required: opinion_exception.uncertain
        }
    ELSE:
        exceptions_applied.APPEND(opinion_exception.exception_section)

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 4: TENDENCY/COINCIDENCE CHECK (s97-98, s101)
// ═══════════════════════════════════════════════════════════════════════════════════════════

IF IS_TENDENCY_OR_COINCIDENCE(evidence_item):
    tendency_check = CHECK_TENDENCY_REQUIREMENTS(evidence_item, case_packet, jurisdiction_config)
    
    // s97/98: Must have significant probative value
    // s101 (criminal): Probative value must substantially outweigh prejudicial effect
    
    IF tendency_check.significant_probative == FALSE:
        RETURN {
            admissible: FALSE,
            reasons: ["Tendency/coincidence evidence lacks significant probative value - s97/98"],
            exclusion_rule: "TENDENCY_s97_98",
            exceptions_applied: [],
            limited_use: NULL,
            probative_value: tendency_check.probative_value,
            prejudicial_effect: tendency_check.prejudicial_effect,
            s137_analysis: NULL,
            s138_analysis: NULL,
            judicial_determination_required: TRUE  // Always judicial for tendency
        }
    
    IF case_packet.matter_type == CRIMINAL AND tendency_check.substantially_outweighs == FALSE:
        RETURN {
            admissible: FALSE,
            reasons: ["Tendency/coincidence - probative value does not substantially outweigh prejudicial effect - s101"],
            exclusion_rule: "TENDENCY_s101",
            exceptions_applied: [],
            limited_use: NULL,
            probative_value: tendency_check.probative_value,
            prejudicial_effect: tendency_check.prejudicial_effect,
            s137_analysis: NULL,
            s138_analysis: NULL,
            judicial_determination_required: TRUE
        }

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 5: IDENTIFICATION EVIDENCE CHECK (s114-116)
// ═══════════════════════════════════════════════════════════════════════════════════════════

IF IS_VISUAL_IDENTIFICATION(evidence_item):
    identification_check = CHECK_IDENTIFICATION_REQUIREMENTS(evidence_item, case_packet)
    
    // s114: Must be made as part of identification parade (with exceptions)
    // s115: Exclusion if identification parade should have been held
    // s116: Directions about identification evidence
    
    IF identification_check.admissible == FALSE:
        RETURN {
            admissible: FALSE,
            reasons: identification_check.reasons,
            exclusion_rule: identification_check.exclusion_section,
            exceptions_applied: [],
            limited_use: NULL,
            probative_value: probative_value,
            prejudicial_effect: identification_check.prejudicial_effect,
            s137_analysis: NULL,
            s138_analysis: NULL,
            judicial_determination_required: TRUE  // Identification always judicial
        }
    
    // If admissible, note that warning/direction required
    IF identification_check.warning_required:
        limited_use = "WARNING REQUIRED: " + identification_check.warning_text

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 6: PRIVILEGE CHECK (s117-134)
// ═══════════════════════════════════════════════════════════════════════════════════════════

privilege_check = CHECK_PRIVILEGE(evidence_item, case_packet)

IF privilege_check.privileged:
    RETURN {
        admissible: FALSE,
        reasons: ["Evidence subject to privilege - " + privilege_check.privilege_type],
        exclusion_rule: privilege_check.section,
        exceptions_applied: [],
        limited_use: NULL,
        probative_value: probative_value,
        prejudicial_effect: 0.0,
        s137_analysis: NULL,
        s138_analysis: NULL,
        judicial_determination_required: privilege_check.waiver_possible
    }

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 7: s137 MANDATORY EXCLUSION (Criminal proceedings only)
// ═══════════════════════════════════════════════════════════════════════════════════════════

s137_analysis = NULL

IF case_packet.matter_type == CRIMINAL:
    // s137: Court MUST refuse to admit if probative value outweighed by
    // danger of unfair prejudice to defendant
    
    prejudicial_effect = ASSESS_PREJUDICIAL_EFFECT(evidence_item, case_packet)
    
    s137_analysis = {
        probative_value: probative_value,
        prejudicial_effect: prejudicial_effect,
        outweighed: prejudicial_effect > probative_value,
        factors_considered: IDENTIFY_PREJUDICE_FACTORS(evidence_item, case_packet)
    }
    
    IF s137_analysis.outweighed:
        RETURN {
            admissible: FALSE,
            reasons: ["Probative value outweighed by prejudicial effect - s137 (mandatory exclusion)"],
            exclusion_rule: "s137_MANDATORY",
            exceptions_applied: exceptions_applied,
            limited_use: NULL,
            probative_value: probative_value,
            prejudicial_effect: prejudicial_effect,
            s137_analysis: s137_analysis,
            s138_analysis: NULL,
            judicial_determination_required: TRUE  // s137 determination is judicial
        }

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 8: s138 IMPROPERLY/ILLEGALLY OBTAINED EVIDENCE
// ═══════════════════════════════════════════════════════════════════════════════════════════

s138_analysis = NULL

IF evidence_item.obtained_improperly OR evidence_item.obtained_illegally:
    // s138: Discretion to exclude improperly/illegally obtained evidence
    // Court must consider desirability of admitting vs undesirability of admitting
    // evidence obtained in improper/illegal manner
    
    s138_analysis = CONDUCT_S138_ANALYSIS(evidence_item, case_packet)
    
    // Factors under s138(3):
    // (a) probative value
    // (b) importance of the evidence
    // (c) nature of the relevant offence
    // (d) gravity of the impropriety/contravention
    // (e) whether impropriety/contravention was deliberate or reckless
    // (f) whether impropriety/contravention was contrary to or inconsistent with a right
    // (g) whether any other proceeding has been or is likely to be taken
    // (h) difficulty of obtaining evidence without impropriety/contravention
    
    IF s138_analysis.exclude_recommended:
        RETURN {
            admissible: FALSE,
            reasons: ["Improperly/illegally obtained - s138 exclusion recommended"],
            exclusion_rule: "s138_DISCRETIONARY",
            exceptions_applied: exceptions_applied,
            limited_use: NULL,
            probative_value: probative_value,
            prejudicial_effect: prejudicial_effect,
            s137_analysis: s137_analysis,
            s138_analysis: s138_analysis,
            judicial_determination_required: TRUE  // s138 determination is judicial
        }

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 9: s135-136 GENERAL DISCRETION
// ═══════════════════════════════════════════════════════════════════════════════════════════

// s135: General discretion to exclude/limit if probative value substantially
// outweighed by danger of unfair prejudice, misleading, or time wasting

// s136: Power to limit use of evidence

discretionary_check = CHECK_DISCRETIONARY_EXCLUSION(evidence_item, case_packet)

IF discretionary_check.exclude:
    RETURN {
        admissible: FALSE,
        reasons: ["Discretionary exclusion - s135"],
        exclusion_rule: "s135_DISCRETIONARY",
        exceptions_applied: exceptions_applied,
        limited_use: NULL,
        probative_value: probative_value,
        prejudicial_effect: discretionary_check.prejudicial_effect,
        s137_analysis: s137_analysis,
        s138_analysis: s138_analysis,
        judicial_determination_required: TRUE
    }

IF discretionary_check.limit_use:
    limited_use = "LIMITED USE DIRECTION REQUIRED: " + discretionary_check.limitation

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 10: EVIDENCE IS ADMISSIBLE
// ═══════════════════════════════════════════════════════════════════════════════════════════

RETURN {
    admissible: TRUE,
    reasons: ["Evidence passes all admissibility tests"],
    exclusion_rule: NULL,
    exceptions_applied: exceptions_applied,
    limited_use: limited_use,
    probative_value: probative_value,
    prejudicial_effect: prejudicial_effect,
    s137_analysis: s137_analysis,
    s138_analysis: s138_analysis,
    judicial_determination_required: limited_use != NULL OR s138_analysis != NULL
}

END FUNCTION
```

