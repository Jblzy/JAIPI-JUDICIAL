## 4.1 Australian Bail Risk Assessment Function

```
=====================================================================================================
FUNCTION: CALCULATE_BAIL_RISK_AU(defendant, criminal_history, charges, jurisdiction)
=====================================================================================================

INPUT:
  defendant: DEFENDANT_OBJECT
  criminal_history: CRIMINAL_HISTORY_OBJECT
  charges: ARRAY[CHARGE]
  jurisdiction: ENUM(NSW, VIC, QLD, WA, SA, TAS, ACT, NT)

OUTPUT:
  bail_assessment: {
    show_cause_required: BOOLEAN,
    unacceptable_risk: BOOLEAN,
    risk_scores: {
      fta_risk: FLOAT,
      offence_risk: FLOAT,
      safety_risk: FLOAT,
      interference_risk: FLOAT
    },
    recommendation: ENUM(UNCONDITIONAL, CONDITIONAL, REFUSE),
    conditions: ARRAY[CONDITION],
    judicial_review_required: BOOLEAN
  }

PROCESS:

  // ───────────────────────────────────────────────────────────────────────────
  // STEP 1: SHOW CAUSE CHECK (NSW, VIC, QLD, WA, SA, ACT, NT)
  // ───────────────────────────────────────────────────────────────────────────

  show_cause_offences = LOAD_SHOW_CAUSE_SCHEDULE(jurisdiction)
  show_cause_required = ANY(charges, charge IN show_cause_offences)

  IF show_cause_required:
    // Show cause offences typically AJPS-AU ineligible
    // But if reached here, assess if cause shown
    cause_shown = ASSESS_SHOW_CAUSE(defendant, charges)
    IF NOT cause_shown:
      RETURN {
        show_cause_required: TRUE,
        unacceptable_risk: TRUE,
        recommendation: REFUSE,
        conditions: [],
        judicial_review_required: TRUE  // Human must confirm refusal
      }

  // ───────────────────────────────────────────────────────────────────────────
  // STEP 2: CALCULATE RISK SCORES
  // ───────────────────────────────────────────────────────────────────────────

  // FTA Risk (s 18(1)(a))
  fta_risk = 0.0
  
  // Prior FTAs heavily weighted
  IF criminal_history.prior_fta_count > 0:
    fta_risk += MIN(criminal_history.prior_fta_count * 0.25, 0.75)
  
  // Community ties
  community_ties_score = CALCULATE_COMMUNITY_TIES(defendant)
  fta_risk += (1 - community_ties_score) * 0.20
  
  // Flight capability
  IF defendant.has_passport AND NOT defendant.is_citizen:
    fta_risk += 0.15
  IF defendant.significant_overseas_connections:
    fta_risk += 0.10
  
  fta_risk = MIN(fta_risk, 1.0)

  // ─────────────────────────────────────────────────────────────────────────

  // Offence Risk (s 18(1)(b))
  offence_risk = 0.0
  
  // Prior offending on bail
  IF criminal_history.prior_offence_on_bail:
    offence_risk += 0.40
  
  // Pattern of offending
  IF criminal_history.prior_convictions_count > 5:
    offence_risk += 0.25
  ELSE IF criminal_history.prior_convictions_count > 2:
    offence_risk += 0.15
  
  // Time since last offence
  IF criminal_history.days_since_last_offence < 30:
    offence_risk += 0.20
  ELSE IF criminal_history.days_since_last_offence < 180:
    offence_risk += 0.10
  
  // Substance abuse
  IF defendant.active_substance_dependency:
    offence_risk += 0.15
  
  offence_risk = MIN(offence_risk, 1.0)

  // ─────────────────────────────────────────────────────────────────────────

  // Safety Risk (s 18(1)(c))
  safety_risk = 0.0
  
  // Victim relationship
  IF ANY(charges, charge.is_domestic_violence):
    safety_risk += 0.40
  IF ANY(charges, charge.victim_is_child):
    safety_risk += 0.35
  
  // Prior violence
  IF criminal_history.prior_violent_offences > 0:
    safety_risk += MIN(criminal_history.prior_violent_offences * 0.15, 0.30)
  
  // Threats made
  IF defendant.has_threatened_victim:
    safety_risk += 0.25
  IF defendant.has_threatened_witness:
    safety_risk += 0.20
  
  // Weapons
  IF ANY(charges, charge.involves_weapon):
    safety_risk += 0.20
  
  safety_risk = MIN(safety_risk, 1.0)

  // ─────────────────────────────────────────────────────────────────────────

  // Interference Risk (s 18(1)(d))
  interference_risk = 0.0
  
  // Known association with witnesses
  IF defendant.known_association_with_witnesses:
    interference_risk += 0.30
  
  // Prior interference/intimidation
  IF criminal_history.prior_witness_intimidation:
    interference_risk += 0.50
  
  // Co-accused at large
  IF case_has_coaccused_at_large:
    interference_risk += 0.20
  
  interference_risk = MIN(interference_risk, 1.0)

  // ───────────────────────────────────────────────────────────────────────────
  // STEP 3: UNACCEPTABLE RISK DETERMINATION
  // ───────────────────────────────────────────────────────────────────────────

  UNACCEPTABLE_THRESHOLD = 0.70  // Configurable per jurisdiction

  unacceptable_risk = FALSE

  IF fta_risk > UNACCEPTABLE_THRESHOLD:
    unacceptable_risk = TRUE
  IF offence_risk > UNACCEPTABLE_THRESHOLD:
    unacceptable_risk = TRUE
  IF safety_risk > UNACCEPTABLE_THRESHOLD:
    unacceptable_risk = TRUE
  IF interference_risk > UNACCEPTABLE_THRESHOLD:
    unacceptable_risk = TRUE

  // ───────────────────────────────────────────────────────────────────────────
  // STEP 4: CONDITION SELECTION
  // ───────────────────────────────────────────────────────────────────────────

  conditions = []

  IF fta_risk > 0.2:
    conditions.APPEND("Report to police station [frequency based on risk]")
  IF fta_risk > 0.4:
    conditions.APPEND("Surrender passport")
  IF fta_risk > 0.5:
    conditions.APPEND("Security deposit of $[amount]")

  IF safety_risk > 0.3:
    conditions.APPEND("No contact with [victim name]")
    conditions.APPEND("Exclusion zone: not within [distance] of [address]")
  IF safety_risk > 0.5:
    conditions.APPEND("Curfew: [time] to [time]")
    IF jurisdiction.supports_electronic_monitoring:
      conditions.APPEND("Electronic monitoring")

  IF interference_risk > 0.3:
    conditions.APPEND("No contact with [witness names]")

  IF offence_risk > 0.4:
    conditions.APPEND("Reside at [approved address]")
    conditions.APPEND("Report to Community Corrections")

  // ───────────────────────────────────────────────────────────────────────────
  // STEP 5: RECOMMENDATION
  // ───────────────────────────────────────────────────────────────────────────

  IF unacceptable_risk AND NOT CAN_MITIGATE_WITH_CONDITIONS(risk_scores, conditions):
    recommendation = REFUSE
    judicial_review_required = TRUE  // Refusal requires human confirmation
  ELSE IF LENGTH(conditions) == 0:
    recommendation = UNCONDITIONAL
    judicial_review_required = FALSE
  ELSE:
    recommendation = CONDITIONAL
    judicial_review_required = FALSE

  // ───────────────────────────────────────────────────────────────────────────
  // STEP 6: INDIGENOUS CONSIDERATION (if applicable)
  // ───────────────────────────────────────────────────────────────────────────

  IF defendant.identifies_as_indigenous:
    // Custody is last resort - reconsider conditions
    IF recommendation == REFUSE:
      alternative_conditions = GENERATE_ENHANCED_CONDITIONS(risk_scores)
      IF CAN_MITIGATE_WITH_CONDITIONS(risk_scores, alternative_conditions):
        recommendation = CONDITIONAL
        conditions = alternative_conditions
        judicial_review_required = TRUE  // Human reviews enhanced conditions

  RETURN {
    show_cause_required: show_cause_required,
    unacceptable_risk: unacceptable_risk,
    risk_scores: {
      fta_risk: fta_risk,
      offence_risk: offence_risk,
      safety_risk: safety_risk,
      interference_risk: interference_risk
    },
    recommendation: recommendation,
    conditions: conditions,
    judicial_review_required: judicial_review_required
  }

END FUNCTION
```

