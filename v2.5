=====================================================================================================
FUNCTION: PROCESS_BAIL_AUSTRALIA(defendant, charges, jurisdiction_config)
=====================================================================================================

INPUT:
    defendant: DEFENDANT_OBJECT
    charges: ARRAY[CHARGE]
    jurisdiction_config: JURISDICTION_CONFIG

OUTPUT:
    bail_decision: {
        determination: ENUM(GRANTED_UNCONDITIONAL, GRANTED_CONDITIONAL, REFUSED),
        conditions: ARRAY[BAIL_CONDITION],
        reasons: STRING,
        review_date: DATETIME or NULL,
        judicial_endorsement_required: BOOLEAN,
        recommendation_only: BOOLEAN
    }

PROCESS:

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 1: DETERMINE APPLICABLE BAIL REGIME
// ═══════════════════════════════════════════════════════════════════════════════════════════

bail_legislation = jurisdiction_config.bail_legislation

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 2: CHECK FOR SHOW CAUSE OFFENCES (NSW, VIC Schedule 2, etc.)
// ═══════════════════════════════════════════════════════════════════════════════════════════

show_cause_required = FALSE
exceptional_circumstances_required = FALSE  // VIC Schedule 1

FOR EACH charge IN charges:
    IF IS_SHOW_CAUSE_OFFENCE(charge, jurisdiction_config):
        show_cause_required = TRUE
    IF IS_EXCEPTIONAL_CIRCUMSTANCES_OFFENCE(charge, jurisdiction_config):  // VIC
        exceptional_circumstances_required = TRUE

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 3: VICTORIA SCHEDULE 1 - EXCEPTIONAL CIRCUMSTANCES (if applicable)
// ═══════════════════════════════════════════════════════════════════════════════════════════

IF exceptional_circumstances_required AND jurisdiction_config.jurisdiction_id == "AU-VIC":
    // Bail must be refused unless exceptional circumstances exist
    // This is an extremely high bar
    
    exceptional_circumstances = ASSESS_EXCEPTIONAL_CIRCUMSTANCES(defendant, charges)
    
    IF exceptional_circumstances.score < THRESHOLDS_AUSTRALIA.VIC_EXCEPTIONAL_CIRCUMSTANCES:
        // Recommend refusal - but this MUST go to human judicial officer
        RETURN {
            determination: REFUSED,
            conditions: [],
            reasons: GENERATE_VIC_SCHEDULE1_REFUSAL_REASONS(exceptional_circumstances),
            review_date: NOW() + 28 DAYS,
            judicial_endorsement_required: TRUE,
            recommendation_only: TRUE  // AJPS cannot make final determination
        }
    ELSE:
        // Proceed to unacceptable risk test
        GOTO STEP_5

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 4: SHOW CAUSE TEST (NSW, VIC Schedule 2, QLD, etc.)
// ═══════════════════════════════════════════════════════════════════════════════════════════

IF show_cause_required:
    // Defendant must show cause why detention is not justified
    
    show_cause_factors = ASSESS_SHOW_CAUSE_FACTORS(defendant, charges)
    
    show_cause_score = CALCULATE_SHOW_CAUSE_SCORE(
        ties_to_community: show_cause_factors.community_ties,
        employment: show_cause_factors.employment,
        family_responsibilities: show_cause_factors.family,
        health_issues: show_cause_factors.health,
        accommodation: show_cause_factors.accommodation,
        time_in_custody: show_cause_factors.time_remanded,
        strength_of_prosecution: show_cause_factors.prosecution_strength,
        previous_compliance: show_cause_factors.bail_history
    )
    
    IF show_cause_score < THRESHOLDS_AUSTRALIA.SHOW_CAUSE_THRESHOLD:
        // Cause not shown - recommend refusal
        RETURN {
            determination: REFUSED,
            conditions: [],
            reasons: GENERATE_SHOW_CAUSE_FAILURE_REASONS(show_cause_factors),
            review_date: NOW() + 28 DAYS,
            judicial_endorsement_required: TRUE,
            recommendation_only: TRUE
        }
    
    // Cause shown - proceed to unacceptable risk test

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 5: UNACCEPTABLE RISK TEST (applies to all bail applications)
// ═══════════════════════════════════════════════════════════════════════════════════════════

STEP_5:

// Assess each risk factor
flight_risk = CALCULATE_FLIGHT_RISK_AUSTRALIA(defendant, charges)
interference_risk = CALCULATE_INTERFERENCE_RISK(defendant, charges)
reoffend_risk = CALCULATE_REOFFEND_RISK_AUSTRALIA(defendant, charges)
endanger_risk = CALCULATE_ENDANGER_RISK(defendant, charges)

// Check if any risk is unacceptable
unacceptable_risk = FALSE
unacceptable_reasons = []

IF flight_risk > THRESHOLDS_AUSTRALIA.FLIGHT_RISK_UNACCEPTABLE:
    unacceptable_risk = TRUE
    unacceptable_reasons.APPEND("Unacceptable risk of failure to appear")

IF interference_risk > THRESHOLDS_AUSTRALIA.INTERFERENCE_RISK_UNACCEPTABLE:
    unacceptable_risk = TRUE
    unacceptable_reasons.APPEND("Unacceptable risk of interference with witnesses/evidence")

IF reoffend_risk > THRESHOLDS_AUSTRALIA.OFFEND_RISK_UNACCEPTABLE:
    unacceptable_risk = TRUE
    unacceptable_reasons.APPEND("Unacceptable risk of committing further offences")

IF endanger_risk > THRESHOLDS_AUSTRALIA.ENDANGER_RISK_UNACCEPTABLE:
    unacceptable_risk = TRUE
    unacceptable_reasons.APPEND("Unacceptable risk of endangering safety of victim/community")

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 6: DETERMINE IF CONDITIONS CAN MITIGATE RISK
// ═══════════════════════════════════════════════════════════════════════════════════════════

IF unacceptable_risk:
    // Check if conditions can reduce risk to acceptable level
    possible_conditions = GENERATE_POSSIBLE_CONDITIONS(
        flight_risk, interference_risk, reoffend_risk, endanger_risk,
        defendant, jurisdiction_config
    )
    
    mitigated_risks = CALCULATE_MITIGATED_RISKS(
        flight_risk, interference_risk, reoffend_risk, endanger_risk,
        possible_conditions
    )
    
    IF mitigated_risks.all_acceptable:
        // Bail can be granted with conditions
        RETURN {
            determination: GRANTED_CONDITIONAL,
            conditions: possible_conditions,
            reasons: GENERATE_CONDITIONAL_BAIL_REASONS(mitigated_risks),
            review_date: NULL,
            judicial_endorsement_required: TRUE,
            recommendation_only: TRUE
        }
    ELSE:
        // Risk cannot be mitigated - refuse bail
        RETURN {
            determination: REFUSED,
            conditions: [],
            reasons: GENERATE_UNACCEPTABLE_RISK_REASONS(unacceptable_reasons),
            review_date: NOW() + 28 DAYS,
            judicial_endorsement_required: TRUE,
            recommendation_only: TRUE
        }

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 7: BAIL GRANTED - DETERMINE CONDITIONS
// ═══════════════════════════════════════════════════════════════════════════════════════════

// No unacceptable risk - bail should be granted
// Determine appropriate conditions

IF ALL(flight_risk < 0.20, interference_risk < 0.20, reoffend_risk < 0.20, endanger_risk < 0.20):
    // Very low risk - unconditional bail
    RETURN {
        determination: GRANTED_UNCONDITIONAL,
        conditions: [
            {type: "APPEAR", description: "Appear at court as required"}
        ],
        reasons: "Low risk assessment - unconditional bail appropriate",
        review_date: NULL,
        judicial_endorsement_required: TRUE,
        recommendation_only: TRUE
    }

ELSE:
    // Some risk - conditional bail
    conditions = GENERATE_PROPORTIONATE_CONDITIONS(
        flight_risk, interference_risk, reoffend_risk, endanger_risk,
        defendant, jurisdiction_config
    )
    
    RETURN {
        determination: GRANTED_CONDITIONAL,
        conditions: conditions,
        reasons: GENERATE_CONDITIONAL_BAIL_REASONS(conditions),
        review_date: NULL,
        judicial_endorsement_required: TRUE,
        recommendation_only: TRUE
    }

END FUNCTION

