=====================================================================================================
FUNCTION: CALCULATE_EVIDENCE_QUALITY(evidence_item, charges, evidence_collection)
=====================================================================================================

INPUT:
    evidence_item: EVIDENCE_OBJECT
    charges: ARRAY[CHARGE]                    // FIXED: Now passed as parameter
    evidence_collection: EVIDENCE_COLLECTION  // FIXED: Now passed as parameter

OUTPUT:
    quality_score: FLOAT (0.0 to 1.0)

PROCESS:

    // ─────────────────────────────────────────────────────────────────────────
    // COMPONENT 1: AUTHENTICITY (0.0 to 1.0) - Weight: 0.25
    // ─────────────────────────────────────────────────────────────────────────
    authenticity = 0.0
    
    IF evidence_item.source_verified:
        authenticity += 0.40
    
    IF evidence_item.tampering_check == PASSED:
        authenticity += 0.30
    ELSE IF evidence_item.tampering_check == INCONCLUSIVE:
        authenticity += 0.15
    // FAILED adds nothing
    
    IF evidence_item.digital_signature_valid:
        authenticity += 0.30
    ELSE IF evidence_item.digital_signature_valid == NULL:
        // No signature to verify (physical evidence)
        authenticity += 0.15  // Partial credit
    
    authenticity = MIN(authenticity, 1.0)
    
    // ─────────────────────────────────────────────────────────────────────────
    // COMPONENT 2: CHAIN OF CUSTODY (0.0 to 1.0) - Weight: 0.25
    // ─────────────────────────────────────────────────────────────────────────
    custody_gaps = COUNT_CUSTODY_GAPS(evidence_item.custody_chain)
    custody_duration = TOTAL_CUSTODY_DURATION(evidence_item.custody_chain)
    
    IF custody_gaps == 0:
        custody_score = 1.0
    ELSE IF custody_gaps == 1 AND custody_duration < 1 HOUR:
        custody_score = 0.85  // Minor gap
    ELSE IF custody_gaps == 1:
        custody_score = 0.70
    ELSE IF custody_gaps == 2:
        custody_score = 0.40
    ELSE:
        custody_score = 0.10
    
    // Verify signatures on custody transfers
    unsigned_transfers = COUNT_UNSIGNED_TRANSFERS(evidence_item.custody_chain)
    IF unsigned_transfers > 0:
        custody_score *= (1 - 0.1 * unsigned_transfers)
    
    custody_score = MAX(custody_score, 0.0)
    
    // ─────────────────────────────────────────────────────────────────────────
    // COMPONENT 3: RELEVANCE (0.0 to 1.0) - Weight: 0.20
    // ─────────────────────────────────────────────────────────────────────────
    // FIXED: Using passed charges parameter
    relevance = CALCULATE_SEMANTIC_RELEVANCE(evidence_item, charges)
    
    // Boost relevance if evidence directly addresses element of offense
    elements_addressed = COUNT_ELEMENTS_ADDRESSED(evidence_item, charges)
    total_elements = COUNT_TOTAL_ELEMENTS(charges)
    element_coverage = elements_addressed / total_elements
    
    relevance = (relevance * 0.6) + (element_coverage * 0.4)
    relevance = MIN(relevance, 1.0)
    
    // ─────────────────────────────────────────────────────────────────────────
    // COMPONENT 4: CORROBORATION (0.0 to 1.0) - Weight: 0.20
    // ─────────────────────────────────────────────────────────────────────────
    // FIXED: Using passed evidence_collection parameter
    corroborating_items = FIND_CORROBORATING_EVIDENCE(evidence_item, evidence_collection)
    
    IF LENGTH(corroborating_items) >= 3:
        corroboration = 1.0
    ELSE IF LENGTH(corroborating_items) == 2:
        corroboration = 0.80
    ELSE IF LENGTH(corroborating_items) == 1:
        corroboration = 0.50
    ELSE:
        corroboration = 0.20
    
    // Weight by quality of corroborating evidence
    IF LENGTH(corroborating_items) > 0:
        avg_corroboration_quality = MEAN([c.quality_score FOR c IN corroborating_items 
                                          IF c.quality_score != NULL])
        IF avg_corroboration_quality != NULL:
            corroboration *= (0.5 + 0.5 * avg_corroboration_quality)
    
    // ─────────────────────────────────────────────────────────────────────────
    // COMPONENT 5: CLARITY (0.0 to 1.0) - Weight: 0.10
    // ─────────────────────────────────────────────────────────────────────────
    SWITCH evidence_item.type:
        CASE VIDEO:
            clarity = ASSESS_VIDEO_CLARITY(evidence_item)
            // Factors: resolution, lighting, obstruction, face visibility
        CASE AUDIO:
            clarity = ASSESS_AUDIO_CLARITY(evidence_item)
            // Factors: noise level, voice clarity, completeness
        CASE DOCUMENT:
            clarity = ASSESS_DOCUMENT_CLARITY(evidence_item)
            // Factors: legibility, completeness, authentication marks
        CASE PHYSICAL:
            clarity = ASSESS_PHYSICAL_CLARITY(evidence_item)
            // Factors: preservation state, uniqueness, identifiability
        CASE DIGITAL:
            clarity = ASSESS_DIGITAL_CLARITY(evidence_item)
            // Factors: data integrity, format readability, metadata completeness
        CASE TESTIMONY:
            clarity = ASSESS_TESTIMONY_CLARITY(evidence_item)
            // Factors: specificity, internal consistency, detail level
        DEFAULT:
            clarity = 0.5  // Unknown type, neutral score
    
    // ─────────────────────────────────────────────────────────────────────────
    // FINAL CALCULATION
    // ─────────────────────────────────────────────────────────────────────────
    quality_score = (authenticity * 0.25) + 
                    (custody_score * 0.25) + 
                    (relevance * 0.20) + 
                    (corroboration * 0.20) + 
                    (clarity * 0.10)
    
    RETURN quality_score

END FUNCTION


=====================================================================================================
FUNCTION: CALCULATE_WITNESS_CREDIBILITY(witness, evidence_collection, challenges)
=====================================================================================================

INPUT:
    witness: WITNESS_OBJECT
    evidence_collection: EVIDENCE_COLLECTION  // FIXED: Now passed as parameter
    challenges: ARRAY[WITNESS_CHALLENGE]      // NEW: Defense challenges to this witness

OUTPUT:
    credibility_score: FLOAT (0.0 to 1.0)

PROCESS:

    // ─────────────────────────────────────────────────────────────────────────
    // COMPONENT 1: INTERNAL CONSISTENCY (0.0 to 1.0) - Weight: 0.25
    // ─────────────────────────────────────────────────────────────────────────
    statements = GET_ALL_STATEMENTS(witness)
    
    IF LENGTH(statements) == 1:
        // Only one statement, check internal consistency
        consistency = MEASURE_INTERNAL_CONSISTENCY(statements[0])
    ELSE:
        // Multiple statements, check cross-statement consistency
        consistency = MEASURE_CROSS_STATEMENT_CONSISTENCY(statements)
    
    // Apply defense challenges re: inconsistency
    inconsistency_challenges = FILTER(challenges, type == INCONSISTENCY)
    FOR EACH challenge IN inconsistency_challenges:
        IF challenge.supported_by_evidence:
            consistency -= 0.15
    
    consistency = MAX(consistency, 0.0)
    
    // ─────────────────────────────────────────────────────────────────────────
    // COMPONENT 2: CORROBORATION WITH EVIDENCE (0.0 to 1.0) - Weight: 0.25
    // ─────────────────────────────────────────────────────────────────────────
    // FIXED: Using passed evidence_collection parameter
    corroboration = MEASURE_EVIDENCE_ALIGNMENT(witness.statement, evidence_collection)
    
    // Check for contradictions with physical evidence
    contradictions = FIND_CONTRADICTIONS(witness.statement, evidence_collection)
    IF LENGTH(contradictions) > 0:
        corroboration -= 0.20 * LENGTH(contradictions)
    
    corroboration = MAX(corroboration, 0.0)
    
    // ─────────────────────────────────────────────────────────────────────────
    // COMPONENT 3: BIAS INDICATORS (0.0 to 1.0, where 1.0 = no bias) - Weight: 0.20
    // ─────────────────────────────────────────────────────────────────────────
    bias_score = 1.0
    
    // Relationship to parties
    IF witness.relationship_to_defendant IN [FAMILY, FRIEND]:
        bias_score -= 0.25
    ELSE IF witness.relationship_to_defendant IN [ADVERSARY]:
        bias_score -= 0.30
    ELSE IF witness.relationship_to_defendant IN [ACQUAINTANCE, COLLEAGUE]:
        bias_score -= 0.10
    
    IF witness.relationship_to_victim IN [FAMILY, FRIEND]:
        bias_score -= 0.20
    ELSE IF witness.relationship_to_victim IN [ACQUAINTANCE, COLLEAGUE]:
        bias_score -= 0.10
    
    // Financial interest
    IF witness.financial_interest_in_outcome:
        bias_score -= 0.30
    
    // Prior conflicts
    IF witness.prior_conflicts_with_defendant:
        bias_score -= 0.20
    
    // Apply defense challenges re: bias
    bias_challenges = FILTER(challenges, type == BIAS)
    FOR EACH challenge IN bias_challenges:
        IF challenge.supported_by_evidence:
            bias_score -= 0.15
    
    bias_score = MAX(bias_score, 0.0)
    
    // ─────────────────────────────────────────────────────────────────────────
    // COMPONENT 4: PERCEPTION CONDITIONS (0.0 to 1.0) - Weight: 0.20
    // ─────────────────────────────────────────────────────────────────────────
    perception = 1.0
    
    // Distance
    IF witness.perception_conditions.distance_from_event > 100 METERS:
        perception -= 0.30
    ELSE IF witness.perception_conditions.distance_from_event > 50 METERS:
        perception -= 0.20
    ELSE IF witness.perception_conditions.distance_from_event > 20 METERS:
        perception -= 0.10
    
    // Lighting
    IF witness.perception_conditions.lighting_conditions == POOR:
        perception -= 0.25
    ELSE IF witness.perception_conditions.lighting_conditions == MODERATE:
        perception -= 0.10
    
    // Intoxication
    IF witness.perception_conditions.intoxication_level > 0:
        perception -= MIN(witness.perception_conditions.intoxication_level * 0.6, 0.5)
    
    // Stress
    IF witness.perception_conditions.stress_level == HIGH:
        perception -= 0.15
    ELSE IF witness.perception_conditions.stress_level == MODERATE:
        perception -= 0.05
    
    // Time since event (memory decay)
    days_since = witness.perception_conditions.time_since_event
    IF days_since > 365:
        perception -= 0.30
    ELSE IF days_since > 90:
        perception -= 0.20
    ELSE IF days_since > 30:
        perception -= 0.15
    ELSE IF days_since > 7:
        perception -= 0.10
    
    // Apply defense challenges re: perception
    perception_challenges = FILTER(challenges, type == PERCEPTION)
    FOR EACH challenge IN perception_challenges:
        IF challenge.supported_by_evidence:
            perception -= 0.10
    
    perception = MAX(perception, 0.0)
    
    // ─────────────────────────────────────────────────────────────────────────
    // COMPONENT 5: PRIOR RELIABILITY (0.0 to 1.0) - Weight: 0.10
    // ─────────────────────────────────────────────────────────────────────────
    IF witness.prior_testimony_history EXISTS AND LENGTH(witness.prior_testimony_history) > 0:
        prior_reliability = witness.prior_testimony_accuracy_rate
        
        // Check for prior perjury or false statements
        IF HAS_PRIOR_PERJURY(witness):
            prior_reliability = 0.1
        ELSE IF HAS_PRIOR_FALSE_STATEMENTS(witness):
            prior_reliability *= 0.5
    ELSE:
        prior_reliability = 0.7  // Neutral default for unknown history
    
    // ─────────────────────────────────────────────────────────────────────────
    // FINAL CALCULATION
    // ─────────────────────────────────────────────────────────────────────────
    credibility_score = (consistency * 0.25) + 
                        (corroboration * 0.25) + 
                        (bias_score * 0.20) + 
                        (perception * 0.20) + 
                        (prior_reliability * 0.10)
    
    RETURN credibility_score

END FUNCTION


=====================================================================================================
FUNCTION: CALCULATE_TRUE_WEIGHTED_MEDIAN(sentences, weights)
=====================================================================================================

INPUT:
    sentences: ARRAY[FLOAT]  // Sentence recommendations from models (months)
    weights: ARRAY[FLOAT]    // Corresponding weights for each model

OUTPUT:
    weighted_median: FLOAT   // The true weighted median sentence

PROCESS:
    
    // ─────────────────────────────────────────────────────────────────────────
    // STEP 1: CREATE AND SORT PAIRS
    // ─────────────────────────────────────────────────────────────────────────
    pairs = []
    FOR i = 0 TO LENGTH(sentences) - 1:
        pairs.APPEND({sentence: sentences[i], weight: weights[i]})
    
    // Sort by sentence ascending
    SORT(pairs, BY: pair.sentence, ORDER: ASCENDING)
    
    // ─────────────────────────────────────────────────────────────────────────
    // STEP 2: CALCULATE TOTAL AND HALF WEIGHT
    // ─────────────────────────────────────────────────────────────────────────
    total_weight = SUM([pair.weight FOR pair IN pairs])
    half_weight = total_weight / 2.0
    
    // ─────────────────────────────────────────────────────────────────────────
    // STEP 3: FIND WEIGHTED MEDIAN
    // ─────────────────────────────────────────────────────────────────────────
    cumulative_weight = 0.0
    
    FOR i = 0 TO LENGTH(pairs) - 1:
        cumulative_weight += pairs[i].weight
        
        IF cumulative_weight >= half_weight:
            // This is the weighted median
            weighted_median = pairs[i].sentence
            BREAK
    
    // ─────────────────────────────────────────────────────────────────────────
    // STEP 4: HANDLE EDGE CASE (exact half weight)
    // ─────────────────────────────────────────────────────────────────────────
    // If cumulative weight exactly equals half weight at position i,
    // the true weighted median is the average of pairs[i].sentence and pairs[i+1].sentence
    // (This is rare but handled for mathematical correctness)
    
    IF cumulative_weight == half_weight AND i < LENGTH(pairs) - 1:
        weighted_median = (pairs[i].sentence + pairs[i+1].sentence) / 2.0
    
    RETURN weighted_median

END FUNCTION

PROOF OF CORRECTNESS:
────────────────────
The weighted median is defined as the value x such that:
    Σ(w_i for all s_i ≤ x) ≥ W/2  AND  Σ(w_i for all s_i ≥ x) ≥ W/2

This algorithm finds the smallest sentence s_k such that the cumulative weight
up to and including s_k is at least half the total weight.

Example verification:
    Sentences: [12, 18, 24, 30, 36] (sorted)
    Weights:   [1.0, 0.8, 1.2, 0.8, 1.0]
    Total weight: 4.8
    Half weight: 2.4
    
    Cumulative after 12: 1.0 (< 2.4, continue)
    Cumulative after 18: 1.8 (< 2.4, continue)
    Cumulative after 24: 3.0 (≥ 2.4, STOP)
    
    Weighted median = 24 ✓


=====================================================================================================
FUNCTION: CALCULATE_PROBABLE_CAUSE_SCORE(evidence_packet, officer_statement)
=====================================================================================================

INPUT:
    evidence_packet: EVIDENCE_COLLECTION
    officer_statement: STRING  // Officer's articulation of probable cause

OUTPUT:
    pc_score: FLOAT (0.0 to 1.0)

PROCESS:
    
    // ─────────────────────────────────────────────────────────────────────────
    // STEP 1: CLASSIFY EVIDENCE BY CLASS
    // ─────────────────────────────────────────────────────────────────────────
    class_a_evidence = FILTER(evidence_packet, classification == CLASS_A)
    class_b_evidence = FILTER(evidence_packet, classification == CLASS_B)
    class_c_evidence = FILTER(evidence_packet, classification == CLASS_C)
    
    // ─────────────────────────────────────────────────────────────────────────
    // STEP 2: CALCULATE NORMALIZED SUPPORT FROM EACH CLASS
    // ─────────────────────────────────────────────────────────────────────────
    // E_A: Normalized support from Class A (direct physical evidence)
    IF LENGTH(class_a_evidence) > 0:
        E_A = MEAN([e.quality_score FOR e IN class_a_evidence])
    ELSE:
        E_A = 0.0
    
    // E_B: Normalized support from Class B (direct digital evidence)
    IF LENGTH(class_b_evidence) > 0:
        E_B = MEAN([e.quality_score FOR e IN class_b_evidence])
    ELSE:
        E_B = 0.0
    
    // E_C: Normalized support from Class C (corroborated testimony)
    IF LENGTH(class_c_evidence) > 0:
        E_C = MEAN([e.quality_score FOR e IN class_c_evidence])
    ELSE:
        E_C = 0.0
    
    // ─────────────────────────────────────────────────────────────────────────
    // STEP 3: CALCULATE PROBABLE CAUSE SCORE
    // ─────────────────────────────────────────────────────────────────────────
    // Formula: PC = 0.50 × E_A + 0.30 × E_B + 0.20 × E_C
    // This weights physical evidence highest, then digital, then testimony
    
    pc_score = (0.50 * E_A) + (0.30 * E_B) + (0.20 * E_C)
    
    // ─────────────────────────────────────────────────────────────────────────
    // STEP 4: OFFICER STATEMENT BONUS (minor)
    // ─────────────────────────────────────────────────────────────────────────
    // A well-articulated officer statement can add a small bonus
    // but cannot create probable cause from nothing
    
    IF officer_statement != NULL:
        statement_quality = ASSESS_OFFICER_STATEMENT_QUALITY(officer_statement)
        // statement_quality is 0.0 to 1.0 based on specificity, consistency, etc.
        
        // Bonus is at most 0.10 and only applies if there's some evidence
        IF pc_score > 0:
            pc_score = MIN(pc_score + (statement_quality * 0.10), 1.0)
    
    RETURN pc_score

END FUNCTION


=====================================================================================================
FUNCTION: CALCULATE_FLIGHT_RISK(defendant, criminal_history)
=====================================================================================================

INPUT:
    defendant: DEFENDANT_OBJECT
    criminal_history: CRIMINAL_HISTORY_OBJECT

OUTPUT:
    flight_risk: FLOAT (0.0 to 1.0)

PROCESS:
    
    risk_score = 0.0
    
    // ─────────────────────────────────────────────────────────────────────────
    // FACTOR 1: TIES TO COMMUNITY
    // ─────────────────────────────────────────────────────────────────────────
    community_ties = 0.0
    
    IF defendant.years_at_current_residence > 5:
        community_ties += 0.20
    ELSE IF defendant.years_at_current_residence > 2:
        community_ties += 0.15
    ELSE IF defendant.years_at_current_residence > 1:
        community_ties += 0.10
    
    IF defendant.has_family_in_jurisdiction:
        community_ties += 0.15
    
    IF defendant.has_dependents:
        community_ties += 0.10
    
    IF defendant.owns_property_in_jurisdiction:
        community_ties += 0.15
    
    // Invert: high community ties = low flight risk
    risk_score += (1.0 - community_ties) * 0.25
    
    // ─────────────────────────────────────────────────────────────────────────
    // FACTOR 2: EMPLOYMENT STATUS
    // ─────────────────────────────────────────────────────────────────────────
    IF defendant.employment_status == STABLE_EMPLOYED:
        employment_factor = 0.0
    ELSE IF defendant.employment_status == SELF_EMPLOYED:
        employment_factor = 0.10
    ELSE IF defendant.employment_status == PART_TIME:
        employment_factor = 0.15
    ELSE IF defendant.employment_status == UNEMPLOYED:
        employment_factor = 0.25
    
    risk_score += employment_factor * 0.15
    
    // ─────────────────────────────────────────────────────────────────────────
    // FACTOR 3: PRIOR FAILURES TO APPEAR
    // ─────────────────────────────────────────────────────────────────────────
    prior_ftas = criminal_history.prior_failures_to_appear
    
    IF prior_ftas == 0:
        fta_factor = 0.0
    ELSE IF prior_ftas == 1:
        fta_factor = 0.40
    ELSE IF prior_ftas == 2:
        fta_factor = 0.70
    ELSE:
        fta_factor = 1.0
    
    risk_score += fta_factor * 0.30  // FTAs heavily weighted
    
    // ─────────────────────────────────────────────────────────────────────────
    // FACTOR 4: FOREIGN TIES / PASSPORT
    // ─────────────────────────────────────────────────────────────────────────
    IF defendant.has_foreign_passport AND NOT defendant.is_citizen:
        foreign_factor = 0.30
    ELSE IF defendant.has_foreign_passport:
        foreign_factor = 0.15
    ELSE IF defendant.has_significant_foreign_ties:
        foreign_factor = 0.20
    ELSE:
        foreign_factor = 0.0
    
    risk_score += foreign_factor * 0.15
    
    // ─────────────────────────────────────────────────────────────────────────
    // FACTOR 5: FINANCIAL RESOURCES
    // ─────────────────────────────────────────────────────────────────────────
    // High resources can mean ability to flee, but also more to lose
    // This is a U-shaped curve: very poor and very wealthy both present risks
    
    IF defendant.estimated_net_worth > 1000000:
        resource_factor = 0.20  // Means to flee
    ELSE IF defendant.estimated_net_worth < 1000:
        resource_factor = 0.15  // Little to lose
    ELSE:
        resource_factor = 0.05  // Middle ground
    
    risk_score += resource_factor * 0.15
    
    // ─────────────────────────────────────────────────────────────────────────
    // NORMALIZE
    // ─────────────────────────────────────────────────────────────────────────
    flight_risk = MIN(risk_score, 1.0)
    
    RETURN flight_risk

END FUNCTION


=====================================================================================================
FUNCTION: CALCULATE_DANGER_RISK(defendant, criminal_history, charges)
=====================================================================================================

INPUT:
    defendant: DEFENDANT_OBJECT
    criminal_history: CRIMINAL_HISTORY_OBJECT
    charges: ARRAY[CHARGE]

OUTPUT:
    danger_risk: FLOAT (0.0 to 1.0)

PROCESS:
    
    risk_score = 0.0
    
    // ─────────────────────────────────────────────────────────────────────────
    // FACTOR 1: CURRENT CHARGE SEVERITY
    // ─────────────────────────────────────────────────────────────────────────
    max_severity = MAX([c.severity FOR c IN charges])
    
    SWITCH max_severity:
        CASE MINOR:
            severity_factor = 0.10
        CASE MODERATE:
            severity_factor = 0.25
        CASE SERIOUS:
            severity_factor = 0.50
        CASE SEVERE:
            severity_factor = 0.80
        CASE CAPITAL:
            severity_factor = 1.0
    
    risk_score += severity_factor * 0.30
    
    // ─────────────────────────────────────────────────────────────────────────
    // FACTOR 2: VIOLENCE INDICATORS IN CURRENT CHARGES
    // ─────────────────────────────────────────────────────────────────────────
    violent_charges = FILTER(charges, is_violent == TRUE)
    
    IF LENGTH(violent_charges) > 0:
        violence_factor = MIN(LENGTH(violent_charges) * 0.25, 0.75)
    ELSE:
        violence_factor = 0.0
    
    // Check for weapon involvement
    IF ANY(charges, involves_weapon == TRUE):
        violence_factor += 0.20
    
    violence_factor = MIN(violence_factor, 1.0)
    risk_score += violence_factor * 0.25
    
    // ─────────────────────────────────────────────────────────────────────────
    // FACTOR 3: CRIMINAL HISTORY
    // ─────────────────────────────────────────────────────────────────────────
    prior_violent = COUNT(criminal_history.prior_convictions WHERE charge.is_violent)
    
    IF prior_violent == 0:
        history_factor = 0.0
    ELSE IF prior_violent == 1:
        history_factor = 0.30
    ELSE IF prior_violent == 2:
        history_factor = 0.50
    ELSE:
        history_factor = 0.70
    
    // Recency matters
    IF criminal_history.time_since_last_offense < 180 DAYS:  // 6 months
        history_factor *= 1.3
    ELSE IF criminal_history.time_since_last_offense < 365 DAYS:  // 1 year
        history_factor *= 1.1
    
    history_factor = MIN(history_factor, 1.0)
    risk_score += history_factor * 0.25
    
    // ─────────────────────────────────────────────────────────────────────────
    // FACTOR 4: VICTIM VULNERABILITY
    // ─────────────────────────────────────────────────────────────────────────
    IF ANY(charges, victim_is_minor == TRUE):
        victim_factor = 0.50
    ELSE IF ANY(charges, victim_is_elderly == TRUE):
        victim_factor = 0.40
    ELSE IF ANY(charges, victim_is_domestic_partner == TRUE):
        victim_factor = 0.45  // DV has high recidivism risk
    ELSE IF ANY(charges, victim_is_known_to_defendant == TRUE):
        victim_factor = 0.25
    ELSE:
        victim_factor = 0.10
    
    risk_score += victim_factor * 0.10
    
    // ─────────────────────────────────────────────────────────────────────────
    // FACTOR 5: THREATS MADE
    // ─────────────────────────────────────────────────────────────────────────
    IF defendant.has_made_threats_to_victim:
        threat_factor = 0.60
    ELSE IF defendant.has_made_threats_to_witnesses:
        threat_factor = 0.50
    ELSE:
        threat_factor = 0.0
    
    risk_score += threat_factor * 0.10
    
    // ─────────────────────────────────────────────────────────────────────────
    // NORMALIZE
    // ─────────────────────────────────────────────────────────────────────────
    danger_risk = MIN(risk_score, 1.0)
    
    RETURN danger_risk

END FUNCTION


=====================================================================================================
FUNCTION: DETERMINE_RELEASE_TYPE(flight_risk, danger_risk, jurisdiction_config)
=====================================================================================================

INPUT:
    flight_risk: FLOAT (0.0 to 1.0)
    danger_risk: FLOAT (0.0 to 1.0)
    jurisdiction_config: JURISDICTION_CONFIG

OUTPUT:
    release_decision: {
        type: ENUM(ROR, SUPERVISED, ELECTRONIC, BAIL, DETENTION),
        conditions: ARRAY[STRING],
        bail_amount: FLOAT or NULL,
        monitoring_level: STRING or NULL
    }

PROCESS:
    
    // ─────────────────────────────────────────────────────────────────────────
    // DETERMINE RELEASE TYPE BASED ON RISK LEVELS
    // ─────────────────────────────────────────────────────────────────────────
    
    // Release on Own Recognizance
    IF flight_risk < THRESHOLDS.FLIGHT_RISK_ROR AND 
       danger_risk < THRESHOLDS.DANGER_RISK_ROR:
        release_type = ROR
        conditions = []
        bail_amount = NULL
        monitoring_level = NULL
    
    // Supervised Release
    ELSE IF flight_risk < THRESHOLDS.FLIGHT_RISK_SUPERVISED AND 
            danger_risk < THRESHOLDS.DANGER_RISK_SUPERVISED:
        release_type = SUPERVISED
        conditions = ["Check-in with pretrial services weekly", 
                      "No travel outside jurisdiction without approval"]
        bail_amount = NULL
        monitoring_level = "STANDARD"
    
    // Electronic Monitoring
    ELSE IF flight_risk < THRESHOLDS.FLIGHT_RISK_ELECTRONIC AND 
            danger_risk < THRESHOLDS.DANGER_RISK_ELECTRONIC:
        release_type = ELECTRONIC
        conditions = ["GPS monitoring", 
                      "Curfew: 8 PM to 6 AM",
                      "Check-in with pretrial services twice weekly"]
        bail_amount = NULL
        monitoring_level = "INTENSIVE"
    
    // Cash Bail (if jurisdiction permits)
    ELSE IF jurisdiction_config.cash_bail_allowed AND
            flight_risk < THRESHOLDS.FLIGHT_RISK_BAIL AND 
            danger_risk < THRESHOLDS.DANGER_RISK_BAIL:
        release_type = BAIL
        bail_amount = CALCULATE_BAIL_AMOUNT(flight_risk, danger_risk, jurisdiction_config)
        conditions = ["No contact with victim",
                      "Surrender passport",
                      "Check-in with pretrial services weekly"]
        monitoring_level = "STANDARD"
    
    // Pretrial Detention
    ELSE IF flight_risk >= THRESHOLDS.DETENTION_THRESHOLD OR 
            danger_risk >= THRESHOLDS.DETENTION_THRESHOLD:
        release_type = DETENTION
        conditions = []
        bail_amount = NULL
        monitoring_level = NULL
    
    // Fallback: Supervised release with high monitoring
    ELSE:
        release_type = SUPERVISED
        conditions = ["Daily check-in with pretrial services",
                      "No travel outside county",
                      "Employment or treatment program required"]
        bail_amount = NULL
        monitoring_level = "HIGH"
    
    // ─────────────────────────────────────────────────────────────────────────
    // ADD UNIVERSAL CONDITIONS
    // ─────────────────────────────────────────────────────────────────────────
    IF release_type != DETENTION:
        conditions.APPEND("Appear at all court dates")
        conditions.APPEND("No new criminal activity")
        conditions.APPEND("Maintain contact information with court")
    
    RETURN {
        type: release_type,
        conditions: conditions,
        bail_amount: bail_amount,
        monitoring_level: monitoring_level
    }

END FUNCTION


=====================================================================================================
FUNCTION: ASSESS_SUPPRESSION_PLAUSIBILITY(motion, evidence_collection)
=====================================================================================================

INPUT:
    motion: SUPPRESSION_MOTION
    evidence_collection: EVIDENCE_COLLECTION

OUTPUT:
    plausibility_score: FLOAT (0.0 to 1.0)

PROCESS:
    
    // ─────────────────────────────────────────────────────────────────────────
    // STEP 1: ASSESS LEGAL BASIS STRENGTH
    // ─────────────────────────────────────────────────────────────────────────
    legal_basis_score = 0.0
    
    SWITCH motion.grounds:
        CASE ILLEGAL_SEARCH:
            // Check if warrant existed
            IF motion.target_evidence.obtained_with_warrant:
                // Harder to suppress
                legal_basis_score = 0.20
                // But check warrant validity
                IF motion.claims_invalid_warrant:
                    legal_basis_score = 0.40
            ELSE:
                // Warrantless search - check exceptions
                IF motion.claims_no_exception_applies:
                    legal_basis_score = 0.60
                ELSE:
                    legal_basis_score = 0.30
        
        CASE COERCED_STATEMENT:
            IF motion.claims_miranda_violation:
                legal_basis_score = 0.50
            IF motion.claims_physical_coercion:
                legal_basis_score = 0.70
            IF motion.claims_psychological_coercion:
                legal_basis_score = 0.40
        
        CASE CHAIN_OF_CUSTODY:
            custody_gaps = COUNT_CUSTODY_GAPS(motion.target_evidence.custody_chain)
            IF custody_gaps > 2:
                legal_basis_score = 0.50
            ELSE IF custody_gaps > 0:
                legal_basis_score = 0.30
            ELSE:
                legal_basis_score = 0.10
        
        CASE TAMPERING:
            IF motion.has_expert_opinion:
                legal_basis_score = 0.60
            ELSE:
                legal_basis_score = 0.30
        
        DEFAULT:
            legal_basis_score = 0.20
    
    // ─────────────────────────────────────────────────────────────────────────
    // STEP 2: ASSESS FACTUAL SUPPORT
    // ─────────────────────────────────────────────────────────────────────────
    factual_support_score = 0.0
    
    IF motion.supporting_documents != NULL AND LENGTH(motion.supporting_documents) > 0:
        factual_support_score += 0.30
    
    IF motion.supporting_witness_statements != NULL AND LENGTH(motion.supporting_witness_statements) > 0:
        factual_support_score += 0.25
    
    IF motion.expert_analysis != NULL:
        factual_support_score += 0.35
    
    IF motion.cites_specific_facts:
        factual_support_score += 0.10
    
    factual_support_score = MIN(factual_support_score, 1.0)
    
    // ─────────────────────────────────────────────────────────────────────────
    // STEP 3: CHECK CASE LAW MATCH
    // ─────────────────────────────────────────────────────────────────────────
    relevant_cases = FIND_RELEVANT_CASE_LAW(motion.grounds, motion.jurisdiction)
    
    IF LENGTH(relevant_cases) > 0:
        case_law_score = ASSESS_CASE_LAW_APPLICABILITY(motion, relevant_cases)
    ELSE:
        case_law_score = 0.30  // No clear precedent either way
    
    // ─────────────────────────────────────────────────────────────────────────
    // STEP 4: CALCULATE OVERALL PLAUSIBILITY
    // ─────────────────────────────────────────────────────────────────────────
    plausibility_score = (legal_basis_score * 0.40) + 
                         (factual_support_score * 0.35) + 
                         (case_law_score * 0.25)
    
    RETURN plausibility_score

END FUNCTION
```

