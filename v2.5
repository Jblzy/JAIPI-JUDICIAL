    indigenous_assessment: {
        applies: BOOLEAN,
        fernando_factors: ARRAY[FACTOR],
        bugmy_factors: ARRAY[FACTOR],
        deprivation_score: FLOAT,
        reduction_recommendation: FLOAT,
        indigenous_court_eligible: BOOLEAN,
        indigenous_court_recommendation: STRING or NULL,
        cultural_considerations: ARRAY[STRING]
    }

PROCESS:

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 1: DETERMINE IF DEFENDANT IDENTIFIES AS ABORIGINAL OR TORRES STRAIT ISLANDER
// ═══════════════════════════════════════════════════════════════════════════════════════════

IF NOT defendant.identifies_as_indigenous:
    RETURN {
        applies: FALSE,
        fernando_factors: [],
        bugmy_factors: [],
        deprivation_score: 0.0,
        reduction_recommendation: 0.0,
        indigenous_court_eligible: FALSE,
        indigenous_court_recommendation: NULL,
        cultural_considerations: []
    }

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 2: ASSESS FERNANDO FACTORS (Background of Deprivation)
// ═══════════════════════════════════════════════════════════════════════════════════════════

fernando_factors = []

// Childhood environment
IF defendant.childhood_in_care OR defendant.stolen_generations_connection:
    fernando_factors.APPEND({
        factor: "REMOVAL_FROM_FAMILY",
        severity: HIGH,
        weight: 0.20,
        description: "Removed from family/community as child or connected to Stolen Generations"
    })

IF defendant.childhood_abuse OR defendant.childhood_neglect:
    fernando_factors.APPEND({
        factor: "CHILDHOOD_TRAUMA",
        severity: HIGH,
        weight: 0.15,
        description: "Experienced abuse or neglect in childhood"
    })

IF defendant.family_violence_exposure:
    fernando_factors.APPEND({
        factor: "FAMILY_VIOLENCE",
        severity: MODERATE_TO_HIGH,
        weight: 0.12,
        description: "Exposed to family violence"
    })

// Educational disadvantage
IF defendant.education_level < YEAR_10:
    fernando_factors.APPEND({
        factor: "EDUCATIONAL_DISADVANTAGE",
        severity: MODERATE,
        weight: 0.08,
        description: "Limited educational opportunities"
    })

// Housing/community disadvantage
IF defendant.from_remote_community OR defendant.housing_instability:
    fernando_factors.APPEND({
        factor: "HOUSING_DISADVANTAGE",
        severity: MODERATE,
        weight: 0.08,
        description: "Housing instability or from remote community with limited services"
    })

// Employment disadvantage
IF defendant.long_term_unemployed AND defendant.limited_employment_opportunities:
    fernando_factors.APPEND({
        factor: "EMPLOYMENT_DISADVANTAGE",
        severity: MODERATE,
        weight: 0.06,
        description: "Limited employment opportunities"
    })

// Substance abuse linked to disadvantage
IF defendant.substance_abuse AND defendant.substance_abuse_linked_to_trauma:
    fernando_factors.APPEND({
        factor: "SUBSTANCE_ABUSE_TRAUMA_LINKED",
        severity: MODERATE,
        weight: 0.08,
        description: "Substance abuse connected to trauma/disadvantage"
    })

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 3: ASSESS BUGMY FACTORS (Intergenerational Trauma)
// ═══════════════════════════════════════════════════════════════════════════════════════════

bugmy_factors = []

// Family history of removal/institutionalisation
IF defendant.parents_removed OR defendant.grandparents_removed:
    bugmy_factors.APPEND({
        factor: "INTERGENERATIONAL_REMOVAL",
        severity: HIGH,
        weight: 0.15,
        description: "Intergenerational trauma from removal policies"
    })

// Family history of incarceration
IF defendant.family_incarceration_history:
    bugmy_factors.APPEND({
        factor: "FAMILY_INCARCERATION",
        severity: MODERATE,
        weight: 0.08,
        description: "Family members experienced incarceration"
    })

// Community trauma
IF defendant.community_trauma_events:
    bugmy_factors.APPEND({
        factor: "COMMUNITY_TRAUMA",
        severity: MODERATE,
        weight: 0.06,
        description: "Community-level trauma events (deaths, violence, etc.)"
    })

// Cultural disconnection
IF defendant.cultural_disconnection:
    bugmy_factors.APPEND({
        factor: "CULTURAL_DISCONNECTION",
        severity: MODERATE,
        weight: 0.06,
        description: "Disconnection from culture, language, country"
    })

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 4: CALCULATE DEPRIVATION SCORE AND REDUCTION RECOMMENDATION
// ═══════════════════════════════════════════════════════════════════════════════════════════

// Combine Fernando and Bugmy factors
all_factors = fernando_factors + bugmy_factors

// Calculate weighted deprivation score
deprivation_score = SUM([f.weight * SEVERITY_MULTIPLIER(f.severity) FOR f IN all_factors])
deprivation_score = MIN(deprivation_score, 1.0)  // Cap at 1.0

// Calculate reduction recommendation
// Per thresholds: Fernando weight 0.15, Bugmy weight 0.10, max combined 0.25
fernando_component = MIN(
    SUM([f.weight FOR f IN fernando_factors]),
    THRESHOLDS_AUSTRALIA.FERNANDO_CONSIDERATION_WEIGHT
)

bugmy_component = MIN(
    SUM([f.weight FOR f IN bugmy_factors]),
    THRESHOLDS_AUSTRALIA.BUGMY_CONSIDERATION_WEIGHT
)

reduction_recommendation = MIN(
    fernando_component + bugmy_component,
    THRESHOLDS_AUSTRALIA.INDIGENOUS_MAX_REDUCTION
)

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 5: ASSESS INDIGENOUS COURT ELIGIBILITY
// ═══════════════════════════════════════════════════════════════════════════════════════════

indigenous_court_eligible = FALSE
indigenous_court_recommendation = NULL

// Check jurisdiction-specific Indigenous court availability
SWITCH jurisdiction_config.jurisdiction_id:
    
    CASE "AU-VIC":
        // Koori Court
        IF jurisdiction_config.koori_court.available:
            IF defendant.willing_to_plead_guilty AND 
               NOT IS_EXCLUDED_OFFENCE(case_packet.charges, "KOORI_COURT"):
                indigenous_court_eligible = TRUE
                indigenous_court_recommendation = "KOORI_COURT"
    
    CASE "AU-QLD":
        // Murri Court
        IF jurisdiction_config.murri_court.available:
            IF defendant.willing_to_plead_guilty AND
               NOT IS_EXCLUDED_OFFENCE(case_packet.charges, "MURRI_COURT"):
                indigenous_court_eligible = TRUE
                indigenous_court_recommendation = "MURRI_COURT"
    
    CASE "AU-NSW":
        // Circle Sentencing
        IF defendant.willing_to_plead_guilty AND
           ELIGIBLE_FOR_CIRCLE_SENTENCING(defendant, case_packet.charges):
            indigenous_court_eligible = TRUE
            indigenous_court_recommendation = "CIRCLE_SENTENCING"
    
    CASE "AU-SA":
        // Nunga Court
        IF defendant.willing_to_plead_guilty:
            indigenous_court_eligible = TRUE
            indigenous_court_recommendation = "NUNGA_COURT"
    
    CASE "AU-WA":
        // Community Courts (limited availability)
        IF defendant.willing_to_plead_guilty AND
           COMMUNITY_COURT_AVAILABLE(defendant.location):
            indigenous_court_eligible = TRUE
            indigenous_court_recommendation = "COMMUNITY_COURT"

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 6: IDENTIFY CULTURAL CONSIDERATIONS
// ═══════════════════════════════════════════════════════════════════════════════════════════

cultural_considerations = []

// Sorry business/cultural obligations
IF defendant.current_sorry_business:
    cultural_considerations.APPEND("Defendant is currently observing Sorry Business - timing considerations apply")

// Connection to country
IF defendant.strong_connection_to_country:
    cultural_considerations.APPEND("Strong connection to country - consider community-based sentences")

// Elder/community role
IF defendant.elder_status OR defendant.community_role:
    cultural_considerations.APPEND("Defendant has significant role in community - impact of imprisonment on community")

// Kinship obligations
IF defendant.significant_kinship_obligations:
    cultural_considerations.APPEND("Significant kinship and family obligations")

// Cultural/ceremonial obligations
IF defendant.ceremonial_obligations:
    cultural_considerations.APPEND("Cultural and ceremonial obligations to consider in sentencing")

// Remote community considerations
IF defendant.from_remote_community:
    cultural_considerations.APPEND("From remote community - imprisonment would cause significant dislocation")

// ═══════════════════════════════════════════════════════════════════════════════════════════
// STEP 7: RETURN ASSESSMENT
// ═══════════════════════════════════════════════════════════════════════════════════════════

RETURN {
    applies: TRUE,
    fernando_factors: fernando_factors,
    bugmy_factors: bugmy_factors,
    deprivation_score: deprivation_score,
    reduction_recommendation: reduction_recommendation,
    indigenous_court_eligible: indigenous_court_eligible,
    indigenous_court_recommendation: indigenous_court_recommendation,
    cultural_considerations: cultural_considerations
}

END FUNCTION
```

