PROCESS:

    // ═══════════════════════════════════════════════════════════════════════════════════════════
    // PHASE 0: PRE-TRIGGER VALIDATION (L0)
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    
    STEP 0.1: WARRANT VALIDITY CHECK
    IF trigger_event.type == WARRANT:
        IF NOT VERIFY_WARRANT_VALIDITY(trigger_event.warrant_details):
            RETURN {status: REJECTED, reason: "Invalid warrant"}
    
    STEP 0.2: STATUTE OF LIMITATIONS CHECK
    FOR EACH charge IN trigger_event.charges:
        IF IS_STATUTE_EXPIRED(charge, trigger_event.timestamp):
            REMOVE charge FROM trigger_event.charges
    
    IF LENGTH(trigger_event.charges) == 0:
        RETURN {status: DISMISSED, reason: "All charges barred by statute of limitations"}
    
    STEP 0.3: DOUBLE JEOPARDY CHECK
    prior_cases = FIND_PRIOR_CASES(trigger_event.suspect_id, trigger_event.charges)
    FOR EACH prior_case IN prior_cases:
        IF prior_case.status IN [ACQUITTED, CONVICTED] AND SAME_OFFENSE(prior_case, trigger_event):
            RETURN {status: DISMISSED, reason: "Double jeopardy - prior adjudication exists"}
    
    STEP 0.4: DIPLOMATIC IMMUNITY CHECK
    IF HAS_DIPLOMATIC_IMMUNITY(trigger_event.suspect_id):
        RETURN {status: RELEASED, reason: "Diplomatic immunity - notify State Department"}
    
    STEP 0.5: JURISDICTION DETERMINATION
    jurisdiction = DETERMINE_JURISDICTION(trigger_event)
    jurisdiction_config = LOAD_JURISDICTION_CONFIG(jurisdiction)
    
    STEP 0.6: AJPS ELIGIBILITY PRE-SCREEN
    FOR EACH charge IN trigger_event.charges:
        IF charge IN jurisdiction_config.ajps_ineligible_offenses:
            RETURN TRANSFER_TO_HUMAN_COURT(trigger_event, 
                   reason: "Charge ineligible for AJPS: " + charge.name)
    
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    // PHASE 1: TRIGGER PROCESSING (L1)
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    
    STEP 1.1: CLASSIFY TRIGGER TYPE
    trigger_classification = CLASSIFY_TRIGGER(trigger_event.type)
    // Returns: CUSTODY_AUTHORIZED, CUSTODY_CONDITIONAL, or INVESTIGATION_ONLY
    
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    // PHASE 1.5: PROBABLE CAUSE & CUSTODY LEGALITY (L1.5)
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    
    STEP 1.5.1: CUSTODY AUTHORIZATION CHECK
    IF trigger_classification == INVESTIGATION_ONLY:
        // SUBPOENA or CITIZEN_REPORT - no immediate custody
        IF trigger_event.type == CITIZEN_REPORT:
            INITIATE_INVESTIGATION(trigger_event)
            RETURN {status: PENDING, reason: "Investigation initiated from citizen report"}
        ELSE IF trigger_event.type == SUBPOENA:
            ISSUE_APPEARANCE_NOTICE(trigger_event.suspect_id, trigger_event.charges)
            RETURN {status: PENDING, reason: "Subpoena issued - awaiting appearance"}
    
    STEP 1.5.2: PROBABLE CAUSE VALIDATION (for IN_FLAGRANTE)
    IF trigger_event.type == IN_FLAGRANTE:
        pc_score = CALCULATE_PROBABLE_CAUSE_SCORE(
            trigger_event.evidence_packet,
            trigger_event.officer_probable_cause_statement
        )
        
        IF pc_score < THRESHOLDS.PROBABLE_CAUSE_MIN:
            RELEASE_SUSPECT(trigger_event.suspect_id)
            LOG_RELEASE(reason: "Insufficient probable cause", pc_score: pc_score)
            RETURN {status: RELEASED, reason: "Probable cause not established"}
    
    STEP 1.5.3: WARRANT SCOPE VERIFICATION (for WARRANT)
    IF trigger_event.type == WARRANT:
        IF NOT WITHIN_WARRANT_SCOPE(trigger_event.charges, trigger_event.warrant_details):
            RETURN {status: REJECTED, reason: "Charges outside warrant scope"}
    
    // Custody authorized - proceed to intake
    
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    // PHASE 2: INTAKE (L2)
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    
    STEP 2.1: IDENTITY VERIFICATION
    verified_identity = VERIFY_IDENTITY(trigger_event.suspect_id)
    IF verified_identity.status == FAILED:
        RETURN {status: PENDING, reason: "Identity verification required"}
    
    defendant = CREATE_DEFENDANT_RECORD(verified_identity)
    criminal_history = PULL_CRIMINAL_HISTORY(defendant)
    
    STEP 2.2: SPECIAL CIRCUMSTANCES CHECK
    // Age check
    IF defendant.age < jurisdiction_config.juvenile_age_limit:
        RETURN TRANSFER_TO_HUMAN_COURT(defendant, trigger_event, 
               reason: "Juvenile defendant - AJPS ineligible")
    
    // Mental health screening
    mental_health_screen = CONDUCT_MENTAL_HEALTH_SCREENING(defendant)
    IF mental_health_screen.concerns_identified:
        ORDER_MENTAL_HEALTH_EVALUATION(defendant)
        defendant.mental_health_status = PENDING_EVALUATION
    
    // Intoxication check
    IF IS_INTOXICATED(defendant):
        DELAY_PROCESSING(defendant, until: SOBER)
        LOG_DELAY(reason: "Defendant intoxication")
    
    STEP 2.3: CHARGE CLASSIFICATION
    severity = CLASSIFY_SEVERITY(trigger_event.charges)
    IF severity == CAPITAL:
        RETURN TRANSFER_TO_HUMAN_COURT(defendant, trigger_event, 
               reason: "Capital offense - AJPS ineligible")
    
    STEP 2.4: RIGHTS NOTIFICATION
    primary_language = DETECT_LANGUAGE(defendant)
    rights_document = GENERATE_RIGHTS_NOTIFICATION(primary_language, jurisdiction_config)
    DELIVER_RIGHTS(defendant, rights_document)
    
    comprehension_verified = VERIFY_COMPREHENSION(defendant, rights_document)
    IF comprehension_verified == FALSE:
        ASSIGN_INTERPRETER(defendant)
        EXTEND_ALL_TIMERS(defendant.case_id, factor: 1.5)
        // Re-verify with interpreter
        comprehension_verified = VERIFY_COMPREHENSION_WITH_INTERPRETER(defendant, rights_document)
        IF comprehension_verified == FALSE:
            RETURN TRANSFER_TO_HUMAN_COURT(defendant, trigger_event,
                   reason: "Unable to verify rights comprehension")
    
    STEP 2.5: TIMER INITIALIZATION
    timing = TIMING_MATRIX[severity]
    timers = INITIALIZE_TIMERS(defendant.case_id, timing)
    
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    // PHASE 2.5: BAIL / PRE-TRIAL RELEASE (L2.5)
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    
    STEP 2.5.1: RISK ASSESSMENT
    flight_risk = CALCULATE_FLIGHT_RISK(defendant, criminal_history)
    danger_risk = CALCULATE_DANGER_RISK(defendant, criminal_history, trigger_event.charges)
    
    STEP 2.5.2: RELEASE DETERMINATION
    release_decision = DETERMINE_RELEASE_TYPE(flight_risk, danger_risk, jurisdiction_config)
    
    IF release_decision.type == DETENTION:
        // Pretrial detention requires human review within 48 hours
        SCHEDULE_HUMAN_DETENTION_REVIEW(defendant, deadline: NOW() + 48 HOURS)
    ELSE:
        PROCESS_RELEASE(defendant, release_decision)
    
    STEP 2.5.3: PROTECTIVE ORDER CONSIDERATION
    IF VICTIM_SAFETY_CONCERN(trigger_event):
        protective_order = GENERATE_PROTECTIVE_ORDER_RECOMMENDATION(trigger_event)
        NOTIFY_VICTIM(trigger_event, protective_order)
    
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    // PHASE 3: DEFENDANT ELECTION (L3)
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    
    STEP 3.1: LAWYER CONTACT WINDOW
    WAIT_FOR_LAWYER_CONTACT(defendant, deadline: timers.lawyer_deadline)
    
    IF LAWYER_CONTACTED(defendant):
        lawyer = GET_LAWYER_DETAILS(defendant)
        INTEGRATE_LAWYER_INTO_AJPS(lawyer, defendant.case_id)
    ELSE:
        lawyer = ASSIGN_AI_LEGAL_AID(defendant)
    
    STEP 3.2: SYSTEM CHOICE PRESENTATION
    election = PRESENT_SYSTEM_CHOICE(defendant, lawyer)
    
    IF election == TRADITIONAL_COURT:
        RETURN TRANSFER_TO_HUMAN_COURT(defendant, trigger_event, 
               reason: "Defendant elected traditional system")
    
    IF election == AJPS:
        RECORD_CONSENT(defendant, timestamp: NOW())
        // Consent is revocable until sentencing
    
    IF election == NO_DECISION:
        // Default to traditional system if no affirmative AJPS election
        RETURN TRANSFER_TO_HUMAN_COURT(defendant, trigger_event,
               reason: "Defendant did not affirmatively elect AJPS")
    
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    // PHASE 3.5: DISCOVERY & CHALLENGE WINDOW (L3.5)
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    
    STEP 3.5.1: DISCOVERY PROVISION
    discovery_packet = PREPARE_DISCOVERY(trigger_event.evidence_packet, defendant)
    PROVIDE_DISCOVERY(lawyer, discovery_packet)
    
    STEP 3.5.2: CHALLENGE WINDOW
    challenges = AWAIT_CHALLENGES(lawyer, deadline: timers.challenge_deadline)
    
    STEP 3.5.3: PROCESS AFFIRMATIVE DEFENSES
    affirmative_defenses = EXTRACT_AFFIRMATIVE_DEFENSES(challenges)
    
    IF INSANITY IN affirmative_defenses OR DIMINISHED_CAPACITY IN affirmative_defenses:
        RETURN TRANSFER_TO_HUMAN_COURT(defendant, trigger_event,
               reason: "Insanity/diminished capacity defense - AJPS ineligible")
    
    STEP 3.5.4: PROCESS DEFENSE EVIDENCE
    defense_evidence = EXTRACT_DEFENSE_EVIDENCE(challenges)
    // Will be integrated in L4
    
    STEP 3.5.5: PREPARE SUPPRESSION MOTIONS
    suppression_motions = EXTRACT_SUPPRESSION_MOTIONS(challenges)
    // Will be processed in L4.5
    
    STEP 3.5.6: PROCESS WITNESS CHALLENGES
    witness_challenges = EXTRACT_WITNESS_CHALLENGES(challenges)
    // Will be integrated into WCAM
    
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    // PHASE 4: EVIDENCE ANALYSIS (L4)
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    
    STEP 4.1: EVIDENCE INGESTION
    evidence_collection = INGEST_EVIDENCE(trigger_event.evidence_packet)
    
    // Integrate defense evidence
    FOR EACH evidence_item IN defense_evidence:
        evidence_item.defense_submitted = TRUE
        evidence_collection.APPEND(evidence_item)
    
    STEP 4.2: EVIDENCE INTEGRITY VERIFICATION
    FOR EACH evidence_item IN evidence_collection:
        integrity_result = VERIFY_EVIDENCE_INTEGRITY(evidence_item)
        IF integrity_result.status == FAILED:
            evidence_item.classification = CLASS_F
            evidence_item.integrity_failure_reason = integrity_result.reason
            FLAG_FOR_HUMAN_REVIEW(evidence_item, reason: "Integrity verification failed")
    
    STEP 4.3: EVIDENCE CLASSIFICATION AND SCORING
    FOR EACH evidence_item IN evidence_collection WHERE classification != CLASS_F:
        evidence_item.classification = CLASSIFY_EVIDENCE(evidence_item)
        evidence_item.quality_score = CALCULATE_EVIDENCE_QUALITY(
            evidence_item, 
            trigger_event.charges, 
            evidence_collection
        )
        evidence_item.weight = EVIDENCE_WEIGHTS[evidence_item.classification]
        
        IF evidence_item.quality_score < THRESHOLDS.EVIDENCE_QUALITY_MIN:
            FLAG_FOR_HUMAN_REVIEW(evidence_item, reason: "Low quality score")
    
    STEP 4.4: WITNESS PROCESSING
    witnesses = EXTRACT_WITNESSES(evidence_collection)
    FOR EACH witness IN witnesses:
        // Apply defense challenges to witness
        applicable_challenges = FIND_APPLICABLE_CHALLENGES(witness, witness_challenges)
        witness.credibility_score = CALCULATE_WITNESS_CREDIBILITY(
            witness, 
            evidence_collection,
            applicable_challenges
        )
        IF witness.credibility_score < THRESHOLDS.WITNESS_CREDIBILITY_MIN:
            FLAG_FOR_HUMAN_REVIEW(witness, reason: "Low credibility score")
    
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    // PHASE 4.5: ADMISSIBILITY ADJUDICATION (L4.5)
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    
    STEP 4.5.1: PROCESS SUPPRESSION MOTIONS
    FOR EACH motion IN suppression_motions:
        plausibility_score = ASSESS_SUPPRESSION_PLAUSIBILITY(motion, evidence_collection)
        
        IF plausibility_score < THRESHOLDS.SUPPRESSION_PLAUSIBILITY_MIN:
            // Motion denied as frivolous
            LOG_SUPPRESSION_DENIAL(motion, reason: "Insufficient plausibility")
        ELSE:
            // Requires human suppression hearing
            suppression_ruling = AWAIT_HUMAN_SUPPRESSION_HEARING(motion, evidence_collection)
            
            IF suppression_ruling == GRANTED:
                // Exclude evidence
                target_evidence = FIND_EVIDENCE(motion.target_evidence_id, evidence_collection)
                target_evidence.classification = CLASS_F
                target_evidence.suppression_reason = motion.grounds
                
                // Fruit of the poisonous tree analysis
                derived_evidence = FIND_DERIVED_EVIDENCE(target_evidence, evidence_collection)
                FOR EACH derived IN derived_evidence:
                    derivation_score = CALCULATE_DERIVATION_SCORE(derived, target_evidence)
                    IF derivation_score > THRESHOLDS.DERIVATION_SCORE_MIN:
                        // Check for exceptions
                        exception_applies = CHECK_EXCLUSION_EXCEPTIONS(derived, target_evidence)
                        IF exception_applies:
                            LOG_EXCEPTION_APPLIED(derived, exception_applies)
                        ELSE:
                            derived.classification = CLASS_F
                            derived.suppression_reason = "Fruit of the poisonous tree"
            
            ELSE IF suppression_ruling == PARTIALLY_GRANTED:
                APPLY_PARTIAL_SUPPRESSION(motion, suppression_ruling.details)
    
    STEP 4.5.2: RECALCULATE EVIDENCE SUFFICIENCY
    admissible_evidence = FILTER(evidence_collection, WHERE classification != CLASS_F)
    aggregate_evidence_score = CALCULATE_AGGREGATE_EVIDENCE(admissible_evidence)
    
    IF aggregate_evidence_score < THRESHOLDS.EVIDENCE_SUFFICIENCY:
        RETURN {
            status: DISMISSED,
            reason: "Insufficient evidence after suppression rulings",
            explanation: GENERATE_INSUFFICIENCY_EXPLANATION(evidence_collection),
            appeal_window: NULL
        }
    
    STEP 4.5.3: COMPLEXITY ASSESSMENT
    complexity_score = CALCULATE_COMPLEXITY(
        charge_count: LENGTH(trigger_event.charges),
        defendant_count: 1,
        evidence_volume: LENGTH(admissible_evidence),
        legal_novelty: ASSESS_LEGAL_NOVELTY(trigger_event.charges),
        jurisdictional_issues: ASSESS_JURISDICTION_COMPLEXITY(trigger_event),
        mental_health: defendant.mental_health_status
    )
    
    IF complexity_score > THRESHOLDS.COMPLEXITY_HUMAN_REQUIRED:
        RETURN TRANSFER_TO_HUMAN_COURT(defendant, trigger_event,
               reason: "Case complexity exceeds AJPS threshold")
    
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    // PHASE 5: AI TRIBUNAL DELIBERATION (L5)
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    
    STEP 5.1: PREPARE CASE PACKET
    case_packet = {
        defendant: defendant,
        charges: trigger_event.charges,
        evidence: admissible_evidence,
        criminal_history: criminal_history,
        mitigating_factors: EXTRACT_MITIGATING_FACTORS(defendant, evidence_collection),
        aggravating_factors: EXTRACT_AGGRAVATING_FACTORS(defendant, evidence_collection),
        victim_impact: EXTRACT_VICTIM_IMPACT(evidence_collection),
        jurisdiction: jurisdiction,
        jurisdiction_config: jurisdiction_config,
        affirmative_defenses: affirmative_defenses,
        witness_credibility_scores: {w.id: w.credibility_score FOR w IN witnesses}
    }
    
    STEP 5.2: MODEL DELIBERATION - PHASE 1 (PARALLEL)
    model_outputs = {}
    
    PARALLEL {
        model_outputs.ALPHA = MODEL_ALPHA.DELIBERATE(case_packet)
        model_outputs.BETA = MODEL_BETA.DELIBERATE(case_packet)
        model_outputs.GAMMA = MODEL_GAMMA.DELIBERATE(case_packet)
        model_outputs.DELTA = MODEL_DELTA.DELIBERATE(case_packet)
        model_outputs.EPSILON = MODEL_EPSILON.DELIBERATE(case_packet)
    }
    
    STEP 5.3: MODEL DELIBERATION - PHASE 2 (SEQUENTIAL)
    // ZETA runs after other models, needs their outputs
    model_outputs.ZETA = MODEL_ZETA.AUDIT(case_packet, model_outputs)
    
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    // PHASE 5.5: VERDICT AGGREGATION (L5.5)
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    
    STEP 5.5.1: PROCEDURAL COMPLIANCE CHECK
    procedural_score = model_outputs.EPSILON.procedural_score
    IF procedural_score < THRESHOLDS.PROCEDURAL_SCORE_MIN:
        RETURN TRANSFER_TO_HUMAN_COURT(defendant, trigger_event,
               reason: "Procedural irregularities detected",
               procedural_score: procedural_score)
    
    STEP 5.5.2: BIAS FLAG CHECK
    IF ANY(model_outputs.ZETA.bias_scores.values() > THRESHOLDS.BIAS_SCORE_CRITICAL):
        RETURN TRANSFER_TO_HUMAN_COURT(defendant, trigger_event,
               reason: "Significant bias detected",
               bias_scores: model_outputs.ZETA.bias_scores)
    
    STEP 5.5.3: EXTRACT VERDICTS AND CONFIDENCES
    verdicts = {}
    confidences = {}
    FOR EACH model_name IN VOTING_MODELS:
        verdicts[model_name] = model_outputs[model_name].verdict
        confidences[model_name] = model_outputs[model_name].confidence
    
    STEP 5.5.4: COUNT VERDICT TYPES
    guilty_count = COUNT(verdicts.values() WHERE value == GUILTY)
    not_guilty_count = COUNT(verdicts.values() WHERE value == NOT_GUILTY)
    insufficient_count = COUNT(verdicts.values() WHERE value == INSUFFICIENT_EVIDENCE)
    
    STEP 5.5.5: APPLY VERDICT RULES
    
    // Rule 1: Any NOT_GUILTY → acquittal (reasonable doubt)
    IF not_guilty_count >= 1:
        FINAL_VERDICT = NOT_GUILTY
        verdict_reason = "Model(s) found reasonable doubt: " + 
                        [m FOR m IN VOTING_MODELS WHERE verdicts[m] == NOT_GUILTY]
        GOTO STEP 7  // Skip to output
    
    // Rule 2: Unanimous GUILTY with high confidence → conviction
    IF guilty_count == 5 AND ALL(confidences.values() >= THRESHOLDS.CONFIDENCE_MIN_GUILT):
        FINAL_VERDICT = GUILTY
        verdict_reason = "Unanimous guilty verdict with high confidence"
        GOTO STEP 6  // Proceed to sentencing
    
    // Rule 3: Unanimous GUILTY but low confidence → human review
    IF guilty_count == 5 AND ANY(confidences.values() < THRESHOLDS.CONFIDENCE_MIN_GUILT):
        RETURN TRANSFER_TO_HUMAN_COURT(defendant, trigger_event,
               reason: "Unanimous guilty but low confidence",
               confidences: confidences)
    
    // Rule 4: Mixed verdict with INSUFFICIENT → human review
    IF insufficient_count >= 1 AND guilty_count > 0:
        RETURN TRANSFER_TO_HUMAN_COURT(defendant, trigger_event,
               reason: "Mixed verdict requiring human judgment",
               verdicts: verdicts)
    
    // Rule 5: All INSUFFICIENT → dismiss
    IF insufficient_count == 5:
        FINAL_VERDICT = DISMISSED
        verdict_reason = "All models found insufficient evidence"
        GOTO STEP 7  // Skip to output
    
    // Fallback: any other combination → human review
    RETURN TRANSFER_TO_HUMAN_COURT(defendant, trigger_event,
           reason: "Verdict combination requires human judgment",
           verdicts: verdicts)
    
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    // PHASE 6: SENTENCING (L6) - Only reached if FINAL_VERDICT == GUILTY
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    
    STEP 6: SENTENCING
    
    STEP 6.1: APPLY BIAS CORRECTIONS
    FOR EACH model_name IN VOTING_MODELS:
        bias_score = model_outputs.ZETA.bias_scores[model_name]
        IF bias_score > THRESHOLDS.BIAS_SCORE_MAX:
            correction_factor = model_outputs.ZETA.correction_factors[model_name]
            model_outputs[model_name].adjusted_sentence = 
                model_outputs[model_name].sentence * (1 - correction_factor)
        ELSE:
            model_outputs[model_name].adjusted_sentence = model_outputs[model_name].sentence
    
    STEP 6.2: MODEL DISAGREEMENT CHECK
    sentences = [model_outputs[m].adjusted_sentence FOR m IN VOTING_MODELS]
    sentence_median = CALCULATE_MEDIAN(sentences)
    sentence_spread = (MAX(sentences) - MIN(sentences)) / sentence_median
    
    IF sentence_spread > THRESHOLDS.MODEL_DISAGREEMENT_MAX:
        RETURN TRANSFER_TO_HUMAN_COURT(defendant, trigger_event,
               reason: "Model disagreement on sentencing exceeds threshold",
               sentences: sentences,
               spread: sentence_spread)
    
    STEP 6.3: CALCULATE TRUE WEIGHTED MEDIAN
    weighted_median_sentence = CALCULATE_TRUE_WEIGHTED_MEDIAN(
        sentences: [model_outputs[m].adjusted_sentence FOR m IN VOTING_MODELS],
        weights: [MODEL_WEIGHTS[m] FOR m IN VOTING_MODELS]
    )
    
    STEP 6.4: STATUTORY BOUNDS CHECK
    statutory_range = GET_STATUTORY_RANGE(trigger_event.charges, jurisdiction_config)
    
    IF weighted_median_sentence < statutory_range.minimum:
        final_sentence = statutory_range.minimum
        bounds_note = "Adjusted to statutory minimum"
    ELSE IF weighted_median_sentence > statutory_range.maximum:
        final_sentence = statutory_range.maximum
        bounds_note = "Adjusted to statutory maximum"
    ELSE:
        final_sentence = weighted_median_sentence
        bounds_note = NULL
    
    STEP 6.5: PROPORTIONALITY CHECK
    historical_sentences = GET_COMPARABLE_SENTENCES(trigger_event.charges, case_packet)
    historical_mean = MEAN(historical_sentences)
    historical_std = STD_DEV(historical_sentences)
    deviation = ABS(final_sentence - historical_mean) / historical_std
    
    IF deviation > THRESHOLDS.SENTENCE_DEVIATION_MAX:
        FLAG_FOR_HUMAN_REVIEW(final_sentence, 
            reason: "Sentence deviates significantly from historical norm",
            deviation: deviation)
        // Continue but flag in output
    
    STEP 6.6: CONSTRUCT SENTENCE OBJECT
    sentence_object = {
        primary_sentence: ROUND(final_sentence),
        sentence_type: DETERMINE_SENTENCE_TYPE(final_sentence, trigger_event.charges),
        facility_recommendation: RECOMMEND_FACILITY(defendant, final_sentence),
        rehabilitation_program: model_outputs.DELTA.rehabilitation_plan,
        parole_eligibility: CALCULATE_PAROLE_ELIGIBILITY(final_sentence, criminal_history),
        restitution: CALCULATE_RESTITUTION(case_packet.victim_impact),
        conditions: GENERATE_CONDITIONS(defendant, trigger_event.charges),
        bounds_note: bounds_note,
        proportionality_deviation: deviation
    }
    
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    // PHASE 7: EXPLAINABILITY & OUTPUT (L7 & L8)
    // ═══════════════════════════════════════════════════════════════════════════════════════════
    
    STEP 7: OUTPUT GENERATION
    
    STEP 7.1: GENERATE EXPLANATION
    explanation_object = {
        plain_language: GENERATE_PLAIN_LANGUAGE_EXPLANATION(
            defendant, trigger_event.charges, FINAL_VERDICT, sentence_object, model_outputs
        ),
        factor_breakdown: GENERATE_FACTOR_BREAKDOWN(model_outputs),
        model_reasoning: {
            ALPHA: model_outputs.ALPHA.reasoning,
            BETA: model_outputs.BETA.reasoning,
            GAMMA: model_outputs.GAMMA.reasoning,
            DELTA: model_outputs.DELTA.reasoning,
            EPSILON: model_outputs.EPSILON.reasoning
        },
        comparable_cases: GET_COMPARABLE_CASES(trigger_event.charges, 5),
        counterfactual_analysis: GENERATE_COUNTERFACTUALS(case_packet, sentence_object),
        appeal_grounds: IDENTIFY_APPEAL_GROUNDS(case_packet, FINAL_VERDICT, sentence_object),
        verdict_reason: verdict_reason
    }
    
    STEP 7.2: GENERATE AUDIT TRAIL
    audit_trail = {
        case_id: defendant.case_id,
        spec_id: SPEC_ID,  // Hash of this specification
        timestamps: COLLECT_ALL_TIMESTAMPS(),
        decision_points: COLLECT_ALL_DECISION_POINTS(),
        evidence_hashes: HASH_ALL_EVIDENCE(evidence_collection),
        model_versions: GET_MODEL_VERSIONS(),
        system_version: "AJPS 2.0",
        jurisdiction: jurisdiction,
        suppression_rulings: GET_ALL_SUPPRESSION_RULINGS(),
        challenges_processed: challenges
    }
    SIGN_CRYPTOGRAPHICALLY(audit_trail)
    STORE_IMMUTABLE(audit_trail)
    
    STEP 7.3: APPLY OUTPUT WATERMARK
    output_watermark = GENERATE_WATERMARK(
        spec_id: SPEC_ID,
        case_id: defendant.case_id,
        model_versions: GET_MODEL_VERSIONS(),
        timestamp: NOW()
    )
    
    STEP 7.4: FINALIZE OUTCOME
    appeal_deadline = NOW() + jurisdiction_config.appeal_deadline DAYS
    
    IF FINAL_VERDICT == GUILTY:
        RETURN {
            status: PROCESSED,
            verdict: {type: GUILTY, reason: verdict_reason},
            sentence: sentence_object,
            explanation: explanation_object,
            appeal_window: appeal_deadline,
            audit_trail_hash: HASH(audit_trail),
            watermark: output_watermark
        }
    ELSE IF FINAL_VERDICT == NOT_GUILTY:
        RETURN {
            status: NOT_GUILTY,
            verdict: {type: NOT_GUILTY, reason: verdict_reason},
            sentence: NULL,
            explanation: explanation_object,
            appeal_window: NULL,  // Prosecution cannot appeal acquittal (double jeopardy)
            audit_trail_hash: HASH(audit_trail),
            watermark: output_watermark
        }
    ELSE IF FINAL_VERDICT == DISMISSED:
        RETURN {
            status: DISMISSED,
            verdict: {type: DISMISSED, reason: verdict_reason},
            sentence: NULL,
            explanation: explanation_object,
            appeal_window: NULL,
            audit_trail_hash: HASH(audit_trail),
            watermark: output_watermark
        }

END FUNCTION
